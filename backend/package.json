{
  "name": "medusa-starter-default",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build && mkdir -p public/admin && cp -r .medusa/client/* public/admin/ && mv public/admin/entry.jsx public/admin/entry.js && node -e \"const fs = require('fs'); const html = fs.readFileSync('public/admin/index.html', 'utf8'); fs.writeFileSync('public/admin/index.html', html.replace('./entry.jsx', '/app/entry.js'));\"",
    "build:railway": "echo 'Starting Railway build...' && medusa build && mkdir -p public/admin && cp -r .medusa/client/* public/admin/ && mv public/admin/entry.jsx public/admin/entry.js && node -e \"const fs = require('fs'); const html = fs.readFileSync('public/admin/index.html', 'utf8'); fs.writeFileSync('public/admin/index.html', html.replace('./entry.jsx', '/app/entry.js'));\" && echo 'Build completed successfully' && ls -la public/admin/ && echo 'Admin files verified' && echo 'Build artifacts ready for deployment'",
    "build:fallback": "echo 'Running fallback build without admin...' && DISABLE_ADMIN=true medusa build && echo 'Fallback build completed'",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "medusa start",
    "dev": "medusa develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/admin-bundler": "2.8.5-snapshot-20250620191520",
