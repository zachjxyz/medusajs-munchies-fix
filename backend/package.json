{
  "name": "medusa-starter-default",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build && mkdir -p public/admin && cp -r .medusa/client/* public/admin/",
    "build:railway": "echo 'Starting Railway build...' && medusa build && mkdir -p public/admin && cp -r .medusa/client/* public/admin/ && echo 'Build completed successfully' && ls -la public/admin/ && echo 'Admin files verified' && echo 'Build artifacts ready for deployment'",
    "build:fallback": "echo 'Running fallback build without admin...' && DISABLE_ADMIN=true medusa build && echo 'Fallback build completed'",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "medusa start",
    "dev": "medusa develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/admin": "^7.1.18",
    "@medusajs/admin-sdk": "2.8.4",
    "@medusajs/cli": "2.8.4",
    "@medusajs/dashboard": "2.8.4",
    "@medusajs/framework": "2.8.4",
    "@medusajs/icons": "2.8.4",
    "@medusajs/js-sdk": "2.8.4",
    "@medusajs/medusa": "2.8.4",
    "@medusajs/types": "2.8.4",
    "@medusajs/ui": "4.0.14",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@react-email/components": "^0.0.25",
    "@react-email/render": "^1.0.1",
    "@sanity/client": "^6.22.1",
    "@tanstack/react-query": "^5.64.2",
    "awilix": "^8.0.1",
    "pg": "^8.13.0",
    "resend": "^4.0.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.8.4",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.2.11"
  },
  "engines": {
    "node": ">=20"
  },
